datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Admin {
  adminName String?
  adminRole EnumAdminAdminRole?
  createdAt DateTime            @default(now())
  id        String              @id @default(cuid())
  updatedAt DateTime            @updatedAt
}

enum EnumAdminAdminRole {
  Option1
}
